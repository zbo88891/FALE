<script>
document.addEventListener("DOMContentLoaded", function() {
  // Swiper è¨­å®š: æ‰‹æ©Ÿæ¿ä¸€å¼µä¸€å¼µé¡¯ç¤º + coverflowæ—‹è½‰æœ¨é¦¬ç‰¹æ•ˆ
  function getOptions() {
    const isMobile = window.innerWidth <= 600;
    return {
      slidesPerView: isMobile ? 1 : 5,
      spaceBetween: isMobile ? 0 : 18,
      loop: true,
      centeredSlides: isMobile ? true : false,
      effect: isMobile ? "coverflow" : "slide",
      coverflowEffect: isMobile ? {
        rotate: 32,
        stretch: 0,
        depth: 120,
        modifier: 1.15,
        slideShadows: true,
      } : undefined,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev'
      },
      pagination: {
        el: '.swiper-pagination',
        clickable: true
      },
      breakpoints: {
        600: {
          slidesPerView: 1,
          centeredSlides: true,
          effect: "coverflow",
          coverflowEffect: {
            rotate: 32,
            stretch: 0,
            depth: 120,
            modifier: 1.15,
            slideShadows: true
          }
        },
        900: { slidesPerView: 3 },
        1100: { slidesPerView: 5 }
      }
    };
  }
  new Swiper('.ent-rg-swiper-slot', getOptions());
  new Swiper('.ent-rg-swiper-live', getOptions());
  new Swiper('.ent-rg-swiper-sport', getOptions());
  new Swiper('.ent-rg-swiper-lottery', getOptions());
});
</script>

