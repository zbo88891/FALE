<script>
(function(){
  // Seamless carousel auto scroll
  const track = document.querySelector('.wc26-global-sport-carousel-track');
  const cards = Array.from(track.children);
  // è¤‡è£½ç¬¬ä¸€å¼µå’Œç¬¬äºŒå¼µåˆ°æœ€å¾Œï¼Œé”åˆ°ç„¡ç¸«è¼ªæ’­
  track.appendChild(cards[0].cloneNode(true));
  track.appendChild(cards[1].cloneNode(true));
  let current = 0;
  let cardCount = cards.length;
  let cardWidth = cards[0].offsetWidth + 14;
  let trackLength = cardCount + 2; // åŸå¡+è¤‡è£½å¡
  function updateCardWidth() {
    cardWidth = cards[0].offsetWidth + 14;
  }
  function scrollCarousel() {
    updateCardWidth();
    current++;
    track.style.transition = 'transform 0.8s cubic-bezier(.75,.2,.3,1)';
    track.style.transform = `translateX(-${current * cardWidth}px)`;
    // è‹¥åˆ°é”è¤‡è£½å¡ç‰‡ï¼Œé‡ç½®è‡³ç¬¬ä¸€å¼µ
    if (current === cardCount) {
      setTimeout(() => {
        track.style.transition = 'none';
        track.style.transform = `translateX(0px)`;
        current = 0;
      }, 820);
    }
  }
  let timer = setInterval(scrollCarousel, 3200);
  window.addEventListener('resize', ()=>{ updateCardWidth(); track.style.transform = `translateX(-${current * cardWidth}px)`; });
})();
</script>
